# crm SDK Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to version control

# Security Configuration
# The SDK supports loading security credentials from environment variables
# with the prefix: CRM_
#
# Security environment variables:
CRM_AUTHORIZATION_SELF_SERVICE=your_authorization_self_service_here

# Debug Configuration
# Enable HTTP request/response logging for debugging
CRM_DEBUG=true
